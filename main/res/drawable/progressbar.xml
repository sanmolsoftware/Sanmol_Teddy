<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape android:shape="rectangle" android:padding="1dp">
		<stroke android:width="1dp" android:color="#FFD8D8D8" /> 
			<gradient android:startColor="#FFFFFF" android:centerColor="#FFFFFF"
				android:centerY="0.75" android:endColor="#FFFFFF" android:angle="270" />
		</shape>
	</item>
	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
			<stroke android:width="1dp" android:color="#FFD8D8D8" /> 
				<gradient android:startColor="#234" android:centerColor="#234"
					android:centerY="0.75" android:endColor="#a24" android:angle="270" />
			</shape>
		</clip>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
			<stroke android:width="1dp" android:color="#FFD8D8D8" /> 
				<gradient android:startColor="#F69559" android:centerColor="#F69559"
					android:centerY="0.75" android:endColor="#FDEBDD" android:angle="270" />
			</shape>
		</clip>
	</item>
</layer-list> 

